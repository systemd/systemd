[Unit]
Description=Generate SSH AF_VSOCK socket
Documentation=man:systemd-ssh-generator(8)

DefaultDependencies=no
Wants=dbus.socket
After=dbus.service
Before=shutdown.target
Conflicts=shutdown.target

[Service]
Type=oneshot
ExecStart={{LIBEXECDIR}}/systemd-ssh-vsock-generator
